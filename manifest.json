{
  "manifest_version": 3,
  "name": "Tab Organizer",
  "version": "1.0.0",
  "description": "A Chrome extension for organizing and grouping tabs efficiently",
  "permissions": [
    "tabs",
    "tabGroups",
    "storage"
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": "icons/icon.svg"
  },
  "icons": {
    "128": "icons/icon.svg"
  },
  "background": {
    "service_worker": "background.js"
  }
} 